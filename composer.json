{
    "name": "alibe/php-geocodes",
    "description": "Script for retrieving and searching information about the ISO-3166 country code",
    "type": "library",
    "keywords": [
        "country code",
        "ISO-3166"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Andrea Liberati",
            "homepage": "https://alibe.ie"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.4",
        "symfony/string": "^5.0",
        "symfony/translation-contracts": "^1.1",
        "symfony/finder": "^4.4",
        "symfony/intl": "^4.0",
        "adbario/php-dot-notation": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "Alibe\\GeoCodes\\": "src/"
        },
        "exclude": [
            "dev",
            "tests"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Alibe\\GeoCodes\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6",
        "phpstan/phpstan": "^1.10",
        "friendsofphp/php-cs-fixer": "^v3.2",
        "squizlabs/php_codesniffer": "^3.7"
    }
}
